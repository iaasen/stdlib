{
    "name": "iaasen/stdlib",
    "description": "A collection of abstract and base classes supporting they way I like to use Zend Framework",
    "license": "proprietary",
    "keywords": [
    ],
    "minimum-stability": "dev",
    "require": {
        "php" : "^7.1",
        "ext-intl": "*",
        "ext-json": "*",
        "zendframework/zend-db": "^2",
        "zendframework/zend-modulemanager": "^2",
        "zendframework/zend-view": "^2",
        "digitalnature/php-ref": "^1",
        "guzzlehttp/guzzle": "^6",
        "swiftmailer/swiftmailer": "^6",
        "phpdocumentor/reflection-docblock" : "^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "Oppned\\": "src/Oppned",
            "Iaasen\\": "src/Iaasen"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "IaasenTest\\": "test/IaasenTest"
        }
    }
}
