{
    "name": "iaasen/stdlib",
    "description": "An adaption to Laminas framework",
    "license": "proprietary",
    "keywords": [],
    "require": {
        "php": "^7.4|^8",
        "ext-intl": "*",
        "ext-json": "*",
        "laminas/laminas-db": "^2",
        "laminas/laminas-dependency-plugin": "^2",
        "laminas/laminas-modulemanager": "^2",
        "laminas/laminas-navigation": "^2",
        "laminas/laminas-view": "^2",
        "digitalnature/php-ref": "^1",
        "guzzlehttp/guzzle": "^6",
        "swiftmailer/swiftmailer": "^6",
        "phpdocumentor/reflection-docblock": "^5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "Oppned\\": "src/Oppned",
            "Iaasen\\": "src/Iaasen"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "IaasenTest\\": "test/IaasenTest"
        }
    }
}
